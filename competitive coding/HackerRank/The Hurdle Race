#include <bits/stdc++.h>

using namespace std;

vector<string> split_string(string);

// Complete the hurdleRace function below.
int hurdleRace(int k, vector<int> height) {
    int m= *max_element(height.begin(),height.end());
    if(m<k)
    {
        return 0;
    }
    else
    {
        return m-k;
    }


}
