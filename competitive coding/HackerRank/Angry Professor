#include <bits/stdc++.h>

using namespace std;

vector<string> split_string(string);

// Complete the angryProfessor function below.
string angryProfessor(int k, vector<int> a) {
   int n=a.size(),count=0;
    for(int i=0;i<n;i++)
    {
       if(a[i]<=0)
       {
           count++;
       }
    }
    if(count>=k)
    {
        return "NO";
    }
    else
    {
        return "YES";
    }


}
